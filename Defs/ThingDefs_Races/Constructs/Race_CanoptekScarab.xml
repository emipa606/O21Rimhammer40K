<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="O21_ConstructBase">
    <defName>ConstructSwarmScarab</defName>
    <label>small canoptek scarab</label>
    <description>A small Necron Canoptek Scarab, they wander around aimlessly till they aquire a target.</description>
    <graphicData>
      <texPath>Things/Necron/Constructs/Scarab/Necron_Scarab</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>0.8</drawSize>
      <shadowData>
        <volume>(0.4, 0.4, 0.4)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <MoveSpeed>8</MoveSpeed>
      <ArmorRating_Blunt>0.80</ArmorRating_Blunt>
      <ArmorRating_Sharp>1.25</ArmorRating_Sharp>
      <ArmorRating_Heat>0.95</ArmorRating_Heat>
      <MeatAmount>0</MeatAmount>
    </statBases>
    <tools>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>6</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>10</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Mechanoid</thinkTreeMain>
      <body>CanoptekScarab</body>
      <hasGenders>false</hasGenders>
      <baseBodySize>1.0</baseBodySize>
      <lifeExpectancy>2750</lifeExpectancy>
      <baseHealthScale>0.5</baseHealthScale>
      <canBePredatorPrey>false</canBePredatorPrey>
      <lifeStageAges>
        <li>
          <def>ConstructFullyFormed</def>
          <minAge>0</minAge>
          <soundWounded>Pawn_Melee_MechanoidSlash_HitBuilding</soundWounded>
          <!--soundDeath>Pawn_Mech_Scyther_Death</soundDeath-->
          <!--soundCall>Pawn_Mech_Scyther_Call</soundCall-->
        </li>
      </lifeStageAges>
      <soundCallIntervalRange>
        <min>2000</min>
        <max>4000</max>
      </soundCallIntervalRange>
      <soundMeleeHitPawn>Pawn_Melee_MechanoidSlash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_MechanoidSlash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_MechanoidSlash_Miss</soundMeleeMiss>
      <ageGenerationCurve>
        <points>
          <li>(1,0)</li>
          <li>(16,100)</li>
          <li>(200,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>O21_Construct</li>
        <li>NecronBasic</li>
      </hediffGiverSets>
    </race>
    <butcherProducts>
      <Necrodermis>1</Necrodermis>
    </butcherProducts>
    <modExtensions>
      <li Class="Rimhammer40k.Necrons.NecronTweaker">
        <tweakNecronResurrect>false</tweakNecronResurrect>
        <tweakCorpseRot>true</tweakCorpseRot>
      </li>
    </modExtensions>
  </ThingDef>
  <ThingDef ParentName="O21_ConstructBase">
    <defName>ConstructScarab</defName>
    <label>canoptek scarab</label>
    <description>A Necron Canoptek Scarab, they wander around aimlessly till they are given work to do. They will passively produce Necrodermis as a byproduct of consuming biomass, which needs to be collected from them.</description>
    <graphicData>
      <texPath>Things/Necron/Constructs/Scarab/Necron_Scarab</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>1.0</drawSize>
      <shadowData>
        <volume>(0.4, 0.4, 0.4)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <MoveSpeed>6.6</MoveSpeed>
      <ArmorRating_Blunt>0.80</ArmorRating_Blunt>
      <ArmorRating_Sharp>1.25</ArmorRating_Sharp>
      <ArmorRating_Heat>0.95</ArmorRating_Heat>
      <MeatAmount>0</MeatAmount>
      <MarketValue>1</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Shearable">
        <woolDef>Necrodermis</woolDef>
        <shearIntervalDays>1.0</shearIntervalDays>
        <woolAmount>40</woolAmount>
      </li>
    </comps>
    <tools>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>1.65</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Animal</thinkTreeMain>
      <thinkTreeConstant>AnimalConstant</thinkTreeConstant>
      <foodType>OmnivoreAnimal, OvivoreAnimal, OmnivoreRoughAnimal, DendrovoreAnimal</foodType>
      <body>CanoptekScarab</body>
      <hasGenders>false</hasGenders>
      <baseBodySize>1.0</baseBodySize>
      <baseHealthScale>1.0</baseHealthScale>
      <trainability>Advanced</trainability>
      <herdAnimal>false</herdAnimal>
      <lifeStageAges>
        <li>
          <def>ConstructFullyFormed</def>
          <minAge>0</minAge>
          <soundWounded>Pawn_Melee_MechanoidSlash_HitBuilding</soundWounded>
          <!--soundDeath>Pawn_Mech_Scyther_Death</soundDeath-->
          <!--soundCall>Pawn_Mech_Scyther_Call</soundCall-->
        </li>
      </lifeStageAges>
      <soundCallIntervalRange>
        <min>2000</min>
        <max>4000</max>
      </soundCallIntervalRange>
      <soundMeleeHitPawn>Pawn_Melee_MechanoidSlash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_MechanoidSlash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_MechanoidSlash_Miss</soundMeleeMiss>
      <ageGenerationCurve>
        <points>
          <li>(1,0)</li>
          <li>(16,100)</li>
          <li>(200,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>O21_Construct</li>
        <li>NecronBasic</li>
      </hediffGiverSets>
    </race>
    <butcherProducts>
      <Necrodermis>5</Necrodermis>
      <GaussCrystalGreen>2</GaussCrystalGreen>
    </butcherProducts>
    <modExtensions>
      <li Class="Rimhammer40k.Necrons.NecronTweaker">
        <tweakNecronResurrect>false</tweakNecronResurrect>
        <tweakCorpseRot>true</tweakCorpseRot>
      </li>
    </modExtensions>
  </ThingDef>
