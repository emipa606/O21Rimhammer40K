<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <JobDef>
    <defName>FillNecronTomb</defName>
    <driverClass>JobDriver_HaulToContainer</driverClass>
    <reportString>hauling TargetA.</reportString>
  </JobDef>
  <JobDef>
    <defName>NecronHibernate</defName>
    <driverClass>O21Toolbox.Needs.JobDriver_Hibernate</driverClass>
    <reportString>hibernating at TargetA.</reportString>
  </JobDef>
  <JobDef>
    <defName>NecronRecharge</defName>
    <driverClass>O21Toolbox.Needs.JobDriver_RechargeEnergy</driverClass>
    <reportString>recharging at TargetA.</reportString>
  </JobDef>
  <JobDef>
    <defName>NecronRechargeEnergyComp</defName>
    <driverClass>O21Toolbox.Needs.JobDriver_RechargeEnergyFromConsumable</driverClass>
    <reportString>recharging from TargetA.</reportString>
    <modExtensions>
      <li Class="O21Toolbox.Needs.ExtraReportStringProperties">
        <extraReportString>recharging TargetB with TargetA.</extraReportString>
      </li>
    </modExtensions>
  </JobDef>
  <JobDef>
    <defName>NecronRefillPowerSource</defName>
    <driverClass>O21Toolbox.Needs.JobDriver_RefillFuelEnergySource</driverClass>
    <reportString>refueling TargetA with TargetB.</reportString>
  </JobDef>
</Defs>