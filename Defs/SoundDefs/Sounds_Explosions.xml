<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--==================== Imperial - Plasma Pistol Explosion Sound =========================-->
  <SoundDef>
    <defName>O21PP_Explosion</defName>
    <context>MapOnly</context>
    <eventNames />
    <maxSimultaneous>1</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Weapons/Ranged/Plasma_Pistol_Impact</clipFolderPath>
          </li>
        </grains>
        <volumeRange>
          <min>30.0</min>
          <max>100.0</max>
        </volumeRange>
        <pitchRange>
          <min>0.9</min>
          <max>1.0</max>
        </pitchRange>
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>
