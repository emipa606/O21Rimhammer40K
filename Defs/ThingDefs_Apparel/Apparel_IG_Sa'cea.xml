<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ====== Clothing ====== -->
  <ThingDef ParentName="CadianUniformBase">
    <!-- Cadian Uniform -->
    <defName>GuevesaUniformSacea</defName>
    <label>gue'vesa uniform (sa'cea)</label>
    <description>A full-body uniform the Gue'vesa most often wear, typically under their armor.</description>
    <graphicData>
      <texPath>Things/Imperial Guard/Gue'vesa/Fatigues/TauFatigues</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <thingCategories Inherit="false">
      <li>TauApparel</li>
    </thingCategories>
    <apparel>
      <wornGraphicPath>Things/Imperial Guard/Gue'vesa/Fatigues/TauFatigues</wornGraphicPath>
      <tags Inherit="false">
        <li>TauApparel</li>
        <li>Sa'ceaGuevesaApparel</li>
      </tags>
    </apparel>
    <recipeMaker>
      <recipeUsers Inherit="False">
        <li>O21_TauBench_Basic</li>
      </recipeUsers>
      <researchPrerequisite>O21_Tau_Allies_HumanApparel</researchPrerequisite>
    </recipeMaker>
  </ThingDef>
