<Defs>
  <!-- ====== Clothing ====== -->
  <ThingDef ParentName="CadianUniformBase">
    <!-- Krieg Uniform -->
    <defName>KriegUniform</defName>
    <label>Krieg Uniform</label>
    <description>A full-body uniform the Death Korps of Krieg most often wear, typically under their armor.</description>
    <graphicData>
      <texPath>Things/Imperial Guard/Krieg/Clothing/DeathKorpsUniform</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <equippedStatOffsets>
      <ToxicSensitivity>-0.70</ToxicSensitivity>
    </equippedStatOffsets>
    <apparel>
      <wornGraphicPath>Things/Imperial Guard/Krieg/Clothing/DeathKorpsUniform</wornGraphicPath>
      <tags Inherit="False">
        <li>ImperialGuardApparel</li>
        <li>KriegApparel</li>
        <li>KriegTrooperApparel</li>
        <li>KriegGrenadierApparel</li>
        <li>KriegPickelTrooperApparel</li>
        <li>KriegTraderApparel</li>
      </tags>
    </apparel>
  </ThingDef>
