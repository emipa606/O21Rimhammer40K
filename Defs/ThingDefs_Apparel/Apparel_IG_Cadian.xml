        <li>Shell</li>
      </layers>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
