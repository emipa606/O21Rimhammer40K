  <ThingDef ParentName="GuardianHelmetBase" Name="EldarHeadgearBase">
    <!-- Eldar Chinstrap -->
    <defName>UlthweHeadgear</defName>
    <label>Ulthwe Headgear</label>
    <description>Some Eldar like to wear this headgear which covers the jaw and often neck, while not particularly effective for defence it does seem to have an unknown purpose.</description>
    <costList>
      <Steel>20</Steel>
    </costList>
    <graphicData>
      <texPath>Things/Eldar/Ulthwe/Hats/Ulthweheadpiece</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
