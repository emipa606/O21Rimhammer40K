  <ThingDef ParentName="ArmorHelmetMakeableBase" Name="GuardianHelmetBase">
    <!-- Guardian Helmet -->
    <defName>UlthweGuardianHelmet</defName>
    <label>Ulthwe Guardian Helmet</label>
    <techLevel>Spacer</techLevel>
    <description>The helmet commonly worn by the Eldar Guardians.</description>
    <graphicData>
      <texPath>Things/Eldar/Ulthwe/Hats/UlthweHelmet_Guardian</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <recipeMaker>
      <recipeUsers Inherit="False">
        <li>O21_EldarBench_Basic</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <researchPrerequisite>O21_Eld_Guardian</researchPrerequisite>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
    </recipeMaker>
    <costList>
      <Steel>4</Steel>
      <Plasteel>60</Plasteel>
    </costList>
    <statBases>
      <Mass>1.8</Mass>
      <WorkToMake>24000</WorkToMake>
      <MaxHitPoints>200</MaxHitPoints>
      <MarketValue>100</MarketValue>
      <ArmorRating_Sharp>1.00</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.36</ArmorRating_Blunt>
      <ArmorRating_Heat>0.9</ArmorRating_Heat>
      <Insulation_Cold>15</Insulation_Cold>
      <Insulation_Heat>5</Insulation_Heat>
      <ToxicSensitivity>-0.30</ToxicSensitivity>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
    <thingCategories>
      <li>EldarApparel</li>
    </thingCategories>
    <apparel>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <tags>
        <li>EldarApparel</li>
        <li>UlthweApparel</li>
        <li>UlthweGuardianApparel</li>
      </tags>
      <wornGraphicPath>Things/Eldar/Ulthwe/Hats/UlthweHelmet_Guardian</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
    </apparel>
  </ThingDef>
