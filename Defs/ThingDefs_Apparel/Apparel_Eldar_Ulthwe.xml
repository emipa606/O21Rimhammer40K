      <MarketValue>100</MarketValue>
      <Mass>0.14</Mass>
      <ArmorRating_Blunt>0.18</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.72</ArmorRating_Sharp>
      <ArmorRating_Heat>0.4</ArmorRating_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.20</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
      <bodyPartGroups>
        <li>Neck</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Eldar/Ulthwe/Hats/Ulthweheadpiece</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>EldarApparel</li>
        <li>UlthweApparel</li>
        <li>UlthweFarseerApparel</li>
        <li>UlthweCivilianApparel</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
