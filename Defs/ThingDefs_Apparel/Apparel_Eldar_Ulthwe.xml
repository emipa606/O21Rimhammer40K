<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ====== Ulthwe Armors ====== -->
  <ThingDef ParentName="ArmorMachineableBase" Name="GuardianArmorBase">
    <!-- Guardian Armour -->
    <defName>UlthweGuardianArmor</defName>
    <label>Ulthwe Guardian Armor</label>
    <techLevel>Spacer</techLevel>
    <description>The armor commonly worn by the Eldar Guardians.</description>
    <graphicData>
      <texPath>Things/Eldar/Ulthwe/Apparel/UlthweGuardian</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>O21_Eld_Guardian</researchPrerequisite>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>O21_EldarBench_Basic</li>
      </recipeUsers>
    </recipeMaker>
    <costList>
      <Steel>8</Steel>
      <Plasteel>60</Plasteel>
    </costList>
    <statBases>
      <Mass>4</Mass>
      <WorkToMake>64000</WorkToMake>
      <MaxHitPoints>600</MaxHitPoints>
      <MarketValue>500</MarketValue>
      <ArmorRating_Sharp>1.08</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.52</ArmorRating_Blunt>
      <ArmorRating_Heat>0.50</ArmorRating_Heat>
      <Insulation_Cold>30</Insulation_Cold>
      <Insulation_Heat>10</Insulation_Heat>
      <ToxicSensitivity>-0.70</ToxicSensitivity>
      <EquipDelay>5</EquipDelay>
    </statBases>
    <thingCategories>
      <li>EldarApparel</li>
    </thingCategories>
    <apparel>
      <tags>
        <li>EldarApparel</li>
        <li>UlthweApparel</li>
        <li>UlthweGuardianApparel</li>
        <li>UlthweCivilianApparel</li>
      </tags>
      <wornGraphicPath>Things/Eldar/Ulthwe/Apparel/UlthweGuardian</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
        <li>Middle</li>
        <li>Shell</li>
      </layers>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
