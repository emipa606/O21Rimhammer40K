  <PawnKindDef Abstract="True" Name="BaseTerranPawnKind">
    <race>Alien_Terran</race>
    <combatPower>40</combatPower>
    <baseRecruitDifficulty>0.50</baseRecruitDifficulty>
    <isFighter>false</isFighter>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <forceNormalGearQuality>true</forceNormalGearQuality>
  </PawnKindDef>
  <PawnKindDef ParentName="BaseTerranPawnKind">
    <defName>Terran</defName>
    <label>Terran</label>
    <defaultFactionType>TerranPlayerColony</defaultFactionType>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>TerranBackstory</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
    <apparelTags Inherit="false">
      <li>CadianApparel</li>
    </apparelTags>
    <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
    <apparelMoney>
      <min>100</min>
      <max>600</max>
    </apparelMoney>
    <apparelRequired Inherit="False">
      <li>CadianUniform</li>
      <li>CadianArmor</li>
    </apparelRequired>
    <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
    <techHediffsChance>0.03</techHediffsChance>
    <techHediffsMoney>
      <min>50</min>
      <max>800</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
    </techHediffsTags>
  </PawnKindDef>
