  <ThingDef ParentName="ResourceBase" Name="GaussCellBase">
    <defName>O21_NecronGaussCell_Green</defName>
    <label>gauss cell (green)</label>
    <description>Consumable cells of energy used by Necrons to recharge. Colour is irellevant other than determining colour of crytsals used to craft.</description>
    <graphicData>
      <texPath>Things/Necron/Item/GaussCell/Green</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>10</MarketValue>
      <Mass>0.025</Mass>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.3</Flammability>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
    <recipeMaker>
      <researchPrerequisite>O21_Nec_Research</researchPrerequisite>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <effectWorking>Cook</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <workAmount>800</workAmount>
      <targetCountAdjustment>5</targetCountAdjustment>
      <productCount>5</productCount>
      <workSkill>Crafting</workSkill>
      <recipeUsers>
        <li>NecronFabricationAltar</li>
      </recipeUsers>
    </recipeMaker>
    <costList>
      <Necrodermis>2</Necrodermis>
      <GaussCrystalGreen>10</GaussCrystalGreen>
    </costList>
    <comps>
      <li Class="O21Toolbox.Needs.CompProperties_EnergySource">
        <isConsumable>true</isConsumable>
        <energyWhenConsumed>0.80</energyWhenConsumed>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>3</glowRadius>
        <glowColor>(0, 255, 0, 0)</glowColor>
      </li>
    </comps>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
