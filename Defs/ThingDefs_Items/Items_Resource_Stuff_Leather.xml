<Defs>
  <ThingDef ParentName="LeatherBase">
    <!-- Tau Leather -->
    <defName>Leather_Tau</defName>
    <label>tau leather</label>
    <description>Tanned, dried, scraped Tau skin. Most people find this material horrifying. Some find it fascinating. While a decent leather, it neither protects nor insulates particularly well.</description>
    <graphicData>
      <color>(143,194,211)</color>
    </graphicData>
    <statBases>
      <MarketValue>5.2</MarketValue>
      <StuffPower_Armor_Sharp>0.64</StuffPower_Armor_Sharp>
      <StuffPower_Insulation_Cold>12</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>12</StuffPower_Insulation_Heat>
    </statBases>
    <stuffProps>
      <color>(143,194,211)</color>
      <commonality>0.0025</commonality>
    </stuffProps>
  </ThingDef>
