<Defs>
  <!-- ========== Tier 1 Small Bunker ========== -->
  <ThingDef ParentName="O21_BuildingBase" Name="BunkerTier1Small">
    <defName>O21_Bunker_Tier1_Small</defName>
    <label>imperial bunker (tier 1, small)</label>
    <description>Imperial bunker, keeps pawns inside safe while letting them fire out. Stored ammo inside may become volatile if the bunker is compromised.</description>
    <thingClass>O21Toolbox.TurretsPlus.Building_Bunker</thingClass>
    <graphicData>
      <texPath>Things/Imperium/Buildings/Bunkers/T1Small</texPath>
      <shaderType>Transparent</shaderType>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <building>
      <ai_combatDangerous>true</ai_combatDangerous>
      <turretGunDef>O21_Gun_Bunker</turretGunDef>
      <turretBurstCooldownTime>2.2</turretBurstCooldownTime>
    </building>
    <uiIconPath>Things/Imperium/Buildings/Bunkers/T1Small</uiIconPath>
    <uiIconScale>1.0</uiIconScale>
    <altitudeLayer>Building</altitudeLayer>
    <stealable>false</stealable>
    <rotatable>true</rotatable>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <designationCategory>Security</designationCategory>
    <size>(2,2)</size>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <tickerType>Normal</tickerType>
    <passability>Impassable</passability>
    <pathCost>4000</pathCost>
    <fillPercent>0.9</fillPercent>
    <hasTooltip>true</hasTooltip>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <Flammability>0.9</Flammability>
      <WorkToBuild>1000</WorkToBuild>
      <Mass>50</Mass>
      <Beauty>0</Beauty>
      <ShootingAccuracyTurret>0.96</ShootingAccuracyTurret>
    </statBases>
    <comps>
      <li Class="O21Toolbox.TurretsPlus.CompProperties_Bunker">
        <compClass>O21Toolbox.TurretsPlus.Comp_Bunker</compClass>
        <pawnCapacity>1</pawnCapacity>
      </li>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>8.1</explosiveRadius>
        <explosiveDamageType>Bomb</explosiveDamageType>
        <chanceNeverExplodeFromDamage>0</chanceNeverExplodeFromDamage>
      </li>
    </comps>
    <costList>
      <WoodLog>50</WoodLog>
    </costList>
    <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
    <researchPrerequisites>
      <li>O21_Imp_Research</li>
    </researchPrerequisites>
  </ThingDef>
